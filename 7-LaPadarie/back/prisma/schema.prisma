// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./db"
}

model Clientes {
  id Int @id @default(autoincrement())
  nome String
  paes Int
  dinheiros Float
}